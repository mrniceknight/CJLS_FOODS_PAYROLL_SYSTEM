<Window x:Class="CJLS_FOODS_PAYROLL_SYSTEM.Views.Form_Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CJLS_FOODS_PAYROLL_SYSTEM.Views"
        mc:Ignorable="d"
        Title="Form_Dashboard" Height="450" Width="800">
    <Grid>
        <TabControl TabStripPlacement="Left">
            <TabItem Header="Employees" Margin="0" FontSize="20">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Payroll" FontSize="20" Margin="0">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Deductions" FontSize="20" Margin="0">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Contributions" FontSize="20" Margin="0">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
